{
  "name": "frontframe",
  "version": "1.0.0",
  "description": "基于webpack打包的前端vue框架搭建",
  "scripts": {
    "dev": "NODE_ENV=development webpack-dev-server --progress",
    "test": "ENV_CONFIG=test NODE_ENV=development webpack-dev-server --progress",
    "pro": "ENV_CONFIG=pro NODE_ENV=development webpack-dev-server --progress",
    "build:dev": "NODE_ENV=production webpack-dev-server --progress",
    "build:test": "ENV_CONFIG=test NODE_ENV=production webpack-dev-server --progress",
    "build": "ENV_CONFIG=pro NODE_ENV=production webpack",
    "dll": "webpack --config webpack.dll.config.js --colors --display-modules"
  },
  "keywords": [
    "frontframe",
    "vue",
    "webpack",
    "element-ui"
  ],
  "author": "PT",
  "license": "ISC",
  "devDependencies": {
    "@babel/core": "^7.9.0",
    "@babel/plugin-transform-runtime": "^7.9.0",
    "@babel/preset-env": "^7.9.0",
    "autoprefixer": "^9.7.4",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^8.1.0",
    "babel-plugin-component": "^1.1.1",
    "babel-plugin-import": "^1.13.0",
    "clean-webpack-plugin": "^3.0.0",
    "copy-webpack-plugin": "^5.1.1",
    "css-loader": "^3.4.2",
    "eslint": "^6.8.0",
    "eslint-config-standard": "^14.1.1",
    "eslint-plugin-import": "^2.20.1",
    "eslint-plugin-node": "^11.0.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.1",
    "eslint-plugin-vue": "^6.2.2",
    "file-loader": "^6.0.0",
    "html-webpack-plugin": "^3.2.0",
    "html-withimg-loader": "^0.1.16",
    "less": "^3.11.1",
    "less-loader": "^5.0.0",
    "mini-css-extract-plugin": "^0.9.0",
    "postcss-loader": "^3.0.0",
    "postcss-pxtorem": "^5.1.1",
    "speed-measure-webpack-plugin": "^1.3.3",
    "style-loader": "^1.1.3",
    "url-loader": "^4.0.0",
    "vue-loader": "^15.9.1",
    "vue-template-compiler": "^2.6.11",
    "webpack": "^4.42.0",
    "webpack-cli": "^3.3.11",
    "webpack-dev-server": "^3.10.3"
  },
  "dependencies": {
    "@babel/runtime": "^7.9.2",
    "@babel/runtime-corejs3": "^7.9.2",
